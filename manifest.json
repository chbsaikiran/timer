{
  "manifest_version": 3,
  "name": "Simple Timer Extension",
  "version": "1.0",
  "description": "A simple timer extension with sound notification",
  "permissions": ["notifications", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 